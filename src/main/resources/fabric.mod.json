{
  "schemaVersion": 1,
  "id": "addon-template",
  "version": "${version}",
  "name": "Ganovism",
  "description": "A couple QOL improvements for Meteor Client made by Ganov with vibecodeing",
  "authors": [
    "Ganov + AI"
  ],
  "contact": {
    "sources": "https://github.com/MeteorDevelopment/meteor-addon-template"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.ganovism.addon.GanovismAddon"
    ]
  },
  "mixins": [
    "addon-template.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}", "1.21.7", "1.21.6"],
    "meteor-client": "*"
  }
}
